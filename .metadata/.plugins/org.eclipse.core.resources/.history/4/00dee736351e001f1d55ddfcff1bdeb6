package com.kosta.exam07;

public class Consumer extends Thread {
	Product product;
	
	public Consumer(Product product) {
		this.product = product;
	}
	
	//제품 소비
	public void run() {
		
		try {
			product.usePdNumber();
			Thread.sleep(100);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
}
