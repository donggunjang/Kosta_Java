package com.kosta.game5;

import javax.swing.JPanel;

public class MyFrame extends JFrame {
	public MyFrame() {
		MyPanel mp = new MyPanel();
		add(mp);
		setSize(500,500);
		setVisible(true);
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new MyFrame();
	}
}
	
