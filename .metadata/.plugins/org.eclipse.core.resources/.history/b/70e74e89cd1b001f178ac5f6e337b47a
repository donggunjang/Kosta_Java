package com.kosta.exam01;

import java.util.Scanner;

//주민번호를 입력 받아서 나이를 계산하고 출력.
public class AgeFromJumin {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String num;
		int age;
		System.out.println("주민번호 입력:000000-0000000");
		num = sc.next();
		
		if(num.substring(num.indexOf("-"), (num.indexOf("-")+2)).equals("3") ||
				num.substring(num.indexOf("-"), (num.indexOf("-")+2)).equals("4")) {
			age = 2024 - (2000 + Integer.parseInt(num.substring(0,3)));
		}
	}
}
