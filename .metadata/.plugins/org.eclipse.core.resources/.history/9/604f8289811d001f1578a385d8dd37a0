package com.kosta.game5;

public class Missile extends GraphicObject {
	boolean launched = false;
	public Missile(String name) {
		super(name);
		y = -200;
	}
	//미사일 생성시에 미사일로 표현할 이미지 파일명을 전달 받아
	//부모생성자인 GraphicObject 생성자에게 전달
	public void update() {
		if(launched) y -= 1;
		if(y < -100) launched = false;
	}
	
